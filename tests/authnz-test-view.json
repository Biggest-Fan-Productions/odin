{
    "webserver": {
        "views/odin/test/authnz": {
            "view": "odin.login",
            "configuration": {
                "expires": {"hours": 72},
                "permissions": ["p2", "p1", "p404"],
                "failure": {
                    "view": "fost.response.401",
                    "configuration": {
                        "schemes": {
                            "Bearer": {}
                        }
                    }
                }
            }
        }
    }
}
