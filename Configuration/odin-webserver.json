{
    "odin": {
        "JWT secret": "<JWT_SECRET>",
        "JWT reset forgotten password secret": "<JWT_RESET_FORGOTTEN_PASSWORD_SECRET>",
        "Facebook": {
            "allowed": [
                "AppID_1",
                "AppID_2"
            ]
        },
        "Facebook API Endpoint": "<FACEBOOK_API_ENDPOINT>",
        "Google": {
            "Client_ID": [
                "<GOOGLE_CLIENT_ID_1>",
                "<GOOGLE_CLIENT_ID_2>"
            ]
        }
    },
    "webserver": {
        "Load": [
            "fostgres",
            "odin-views"
        ],
        "hosts": {
            "": "root"
        },
        "views": {
            "root": {
                "view": "fost.middleware.logging",
                "configuration": {
                    "view": "fost.middleware.request",
                    "configuration": {
                        "headers": {
                            "__pgdsn": {
                                "dbname": "odin-test"
                            }
                        },
                        "view": "fost.view.pathprefix",
                        "configuration": {
                            "": "redirect",
                            "favicon.ico": "fost.response.404",
                            "odin/": "odin"
                        }
                    }
                }
            },
            "redirect": {
                "view": "fost.response.302",
                "configuration": {
                    "location": "/odin/"
                }
            }
        }
    }
}