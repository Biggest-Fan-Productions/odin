{
    "webserver": {
        "views/odin/app": {
            "view": "fost.view.pathprefix",
            "configuration": {
                "": {
                    "view": "fost.static",
                    "configuration": {
                        "root": "usr/share/odin/Static/app/"
                    }
                },
                "login/": {
                    "view": "odin.app.login",
                    "configuration": {
                        "expires": {"hours": 72},
                        "static": "usr/share/odin/Static/app/login.html"
                    }
                }
            }
        }
    }
}
