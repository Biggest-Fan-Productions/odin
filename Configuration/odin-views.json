{
    "webserver": {
        "views/odin": {
            "view": "fost.view.pathprefix",
            "configuration": {
                "": "odin/static",
                "api/login/": "odin/login"
            }
        },
        "views/odin/static": {
            "view": "fost.static",
            "configuration": {
                "root": "Static/odin"
            }
        },
        "views/odin/login": {
            "view": "odin.login",
            "configuration": {
                "dbname": "odin-test",
                "expires": {"hours": 2}
            }
        }
    }
}
